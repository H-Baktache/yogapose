{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    { 
      "src": "/(.*)\\.js$",
      "dest": "/$1.js",
      "headers": {
        "Content-Type": "application/javascript; charset=utf-8"
      }
    },
    { 
      "src": "/(.*)\\.mjs$", 
      "dest": "/$1.mjs",
      "headers": {
        "Content-Type": "application/javascript; charset=utf-8"
      }
    },
    { "src": "/(.*)", "dest": "/" }
  ],
  "github": {
    "silent": true
  },
  "env": {
    "VITE_GEMINI_API_KEY": "AIzaSyCvYPAEcFKrjb0sgZgq0NoVvWhy9Y_S_cc"
  }
} 